{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                //"${fileDirname}/k_means_clustering/**.cpp",
                //"${fileDirname}/svm/**.cpp",
                "${fileDirname}/neural_nets/**.cpp",
                "-pedantic-errors",
                "-Wall",
                "-Wextra",
                "-std=c++17",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I /usr/local/include/eigen3/"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}